local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "冰脚本1.2", HidePremium = false,SaveConfig = true,IntroText = "冰脚本1.2",ConfigFolder = "bingsex"})

OrionLib:MakeNotification({
	Name = "冰脚本",
